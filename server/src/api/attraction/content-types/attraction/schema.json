{
  "kind": "collectionType",
  "collectionName": "attractions",
  "info": {
    "singularName": "attraction",
    "pluralName": "attractions",
    "displayName": "Attraction"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "cover_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "illustrations": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "tour_program": {
      "type": "text"
    },
    "service": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "Attraction"
    },
    "tour_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tour-type.tour-type",
      "mappedBy": "attractions"
    }
  }
}
